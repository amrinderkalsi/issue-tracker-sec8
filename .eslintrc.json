{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "react/prefer-stateless-function": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
